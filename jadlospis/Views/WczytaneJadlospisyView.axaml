<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:vm="clr-namespace:jadlospis.ViewModels"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="jadlospis.Views.WczytaneJadlospisyView"
             x:DataType="vm:WczytaneJadlospisyViewModel">
    <StackPanel>
        <TextBlock Text="{Binding Name}"></TextBlock>
        <StackPanel Orientation="Horizontal">
            <Button Command="{Binding OpenJadlospis}">Wczytaj</Button>
            <Button Command="{Binding CopyJadlospis}">Kopiuj</Button>
            <Button Command="{Binding DeleteJadlospis}">Usu≈Ñ</Button>
        </StackPanel>
    </StackPanel>
</UserControl>
